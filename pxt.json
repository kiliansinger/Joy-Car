{
    "name": "Joy-Car",
    "version": "1.0.11",
    "description": "MakeCode package for Joy-IT Joy-Car with bluetooth",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "bluetooth": "*",
         "neopixel": "github:microsoft/pxt-neopixel#v0.7.3"
    },
    "files": [
        "README.md",
        "joycar.ts",
        "_locales/de/Joy-Car-strings.json",
        "_locales/de/Joy-Car-jsdoc-strings.json"
    ],
    "testFiles": [
        "test.ts"
    ],
    "supportedTargets": [
        "microbit"
    ],
     "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 1
                }
            }
        }
    }
}
